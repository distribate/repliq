{
  "name": "database",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "npx supabase start",
    "stop": "npx supabase stop",
    "status": "npx supabase status",
    "gen-types": "npx supabase gen types typescript --local > ../../packages/types/entities/supabase.ts",
    "edge": "npx supabase functions serve --env-file \"supabase/.env\"",
    "dump-seed": "npx supabase db dump supabase/seed.sql --data-only --local",
    "dump-schema": "npx supabase db dump supabase/schema.sql --local",
    "start-edge": "npx supabase start && supabase functions serve"
  },
  "devDependencies": {
    "supabase": "^1.187.10"
  }
}
