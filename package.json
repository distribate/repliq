{
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev --filter=skin-proxy --filter=web --filter=authorization --filter=forum-backend",
    "docker-start": "concurrently \"docker compose -f docker-compose.yml up -d\" \"docker compose -f docker-compose-external.yml up -d\"",
    "docker-down": "concurrently \"docker compose -f docker-compose.yml down\" \"docker compose -f docker-compose-external.yml down\"",
    "start": "turbo start --filter=web --filter=authorization --filter=skin-proxy --filter=web --filter=forum-backend",
    "db-dump-schema": "yarn workspace database run dump-schema",
    "auth": "yarn workspace authorization dev",
    "db-dump-seed": "yarn workspace database run dump-seed",
    "db-gen-types": "yarn workspace database run gen-types",
    "lint": "turbo lint",
    "type-check": "turbo type-check",
    "clean": "turbo clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lp-gen-types": "kysely-codegen --dialect postgres --url=env\\(LP_DATABASE_URL\\) --out-file=packages/types/db/luckperms-database-types.ts",
    "auth-gen-types": "kysely-codegen --dialect postgres --url=env\\(AUTH_DATABASE_URL\\) --out-file=packages/types/db/auth-database-types.ts",
    "forum-gen-types": "kysely-codegen --dialect postgres --url=env\\(FORUM_DATABASE_URL\\) --out-file=packages/types/db/forum-database-types.ts",
    "payments-gen-types": "kysely-codegen --dialect postgres --url=env\\(PAYMENTS_DATABASE_URL\\) --out-file=packages/types/db/payments-database-types.ts"
  },
  "dependencies": {
    "@tanstack/react-query": "5.62.3"
  },
  "devDependencies": {
    "@tanstack/react-query-devtools": "5.62.3",
    "concurrently": "9.1.0",
    "cross-env": "7.0.3",
    "dotenv-cli": "7.4.4",
    "eslint": "9.16.0",
    "kysely-codegen": "0.17.0",
    "prettier": "3.4.2",
    "prettier-plugin-tailwindcss": "0.5.14",
    "turbo": "2.3.3"
  },
  "engines": {
    "node": ">=18"
  },
  "name": "fasberry-forum",
  "packageManager": "yarn@4.3.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
