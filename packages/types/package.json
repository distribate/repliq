{
  "name": "@repo/types",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "gen-types": "dotenv -e .env npx supabase gen types --lang typescript --db-url %FORUM_DATABASE_URL% > ./entities/gen-supabase.ts",
    "auth-gen-types": "kysely-codegen --dialect postgres --url=env\\(AUTH_DATABASE_URL\\) --out-file=./db/auth-database-types.ts",
    "forum-gen-types": "kysely-codegen --dialect postgres --url=env\\(FORUM_DATABASE_URL\\) --out-file=./db/forum-database-types.ts",
    "payments-gen-types": "kysely-codegen --dialect postgres --url=env\\(PAYMENTS_DATABASE_URL\\) --out-file=./db/payments-database-types.ts",
    "skins-gen-types": "kysely-codegen --dialect mysql --url=env\\(SKINS_DATABASE_URL\\) --out-file=./db/skins-database-types.ts",
    "lands-gen-types": "kysely-codegen --dialect mysql --url=env\\(LANDS_DATABASE_URL\\) --out-file=./db/lands-database-types.ts",
    "cmi-gen-types": "kysely-codegen --dialect mysql --url=env\\(CMI_DATABASE_URL\\) --out-file=./db/cmi-database-types.ts",
    "playerpoints-gen-types": "kysely-codegen --dialect mysql --url=env\\(PLAYER_POINTS_DATABASE_URL\\) --out-file=./db/player-points-database-types.ts"
  },
  "dependencies": {
    "supabase": "1.219.2",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "dotenv-cli": "7.4.4",
    "kysely-codegen": "0.17.0"
  }
}
